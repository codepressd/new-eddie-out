        <ul>
            <li>Username: </li>
            <li>Email: <%= user.email %></li>
            <li class ="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Favorites 
                <span class="caret"></span></a>
                <ul class="dropdown-menu customDrop">
                    <li><a href="#">HTML</a></li>
                    <li><a href="#">CSS</a></li>
                    <li><a href="#">JavaScript</a></li>
                </ul>
            </li>
        </ul>
    </div>        

 </div>
